    <pre id="msg"></pre>
    <script src="battlecab/camo.js"></script>
<script>
    $(function() {
	function appendMessage(data) {
	    console.log("data: ", data);
	    console.log("msgs: ", data.msgs);
	    jQuery.each(data.msgs, function(i, j) {
		ee = JSON.parse(j);
		console.log("ee: ", i, j, ee);
                if ( ee.mode == "event" ) {                                                                                                                                                                          
                    $('#msg').prepend( "<div class='msg event'><p><span class='event_icon " + ee.color + "'>" + ee.icon + "</span><span class='event_message'>" + ee.msg + "</span></p></div>");       
                } else if ( ee.mode == "comment" ) {																	
                    $('#msg').prepend("<div class='msg'><p class='comment'>" + ee.comment + "</p></div>");                                                  
                } else if ( ee.mode == "choice" ) {                                                                                                                                                                  
                    $('#msg').prepend("<div class='msg choice'><h3 class='choice_message'>" + ee.choice + "</h3>" + ee.choices);                                                                                     
                }                                                                                                                                                                                                    
            });
	    console.log("state", data.state);
		<% CONF["battlecab"]["stats"].each_pair do |e, s| %>
		$("#<%= e %>").val(data.state.<%= e %>);
		<% end %>
		$('#rank').css('background-color', data.state.team);
	    $('#rank').css('color', data.state.color);
	    $('#rank').css('border-left-color',  + data.state.spec);
	    $('#rank').css('border-bottom-color', data.state.spec);
	    $('#rank').css('border-right-color', data.state.spec);
	    return data
	}
	
	    <% if CONF[settings.conf[:subdomain]][:mode] == "stream" %>
	    var src = new EventSource('/s/<%= session[:user] %>');
	src.onmessage = function(e) { j = JSON.parse(e.data); appendMessage(j); }
	    <% else %>
	    setInterval( function() {
		jQuery.get( "/m/<%= session[:user] %>", function(e) { console.log("DATA", e, appendMessage(e)); });
	    } , 1000000);
	jQuery.get( "/m/<%= session[:user] %>", function(e) { console.log("HANDSHAKE", e, appendMessage(e)); });
	    <% end %>
    });
</script>
